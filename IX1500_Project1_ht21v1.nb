Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"n", "=", "1000"}], ";"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "the", " ", "number", " ", "of", " ", "possible", " ", "outcomes", " ", 
    "in", " ", "this", " ", "simulation"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalOmega]", "=", 
   RowBox[{"Sort", "/@", 
    RowBox[{"Table", "[", 
     RowBox[{"hand", ",", 
      RowBox[{"{", "n", "}"}]}], "]"}]}]}], ";"}]}], "Input",ExpressionUUID->\
"45c1a27a-edd2-4c25-9214-b08d825dec58"],

Cell[TextData[{
 "Assume that we want to estimate the probability of straight. We can then \
reuse our ",
 StyleBox["straightQ", "Output"],
 " method."
}], "Text",ExpressionUUID->"26609e6d-4270-4da1-8c9b-61fea1e3661d"],

Cell[BoxData[
 RowBox[{
  RowBox[{"straightQ", "[", "h_", "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"test", "=", 
      RowBox[{"h", "-", 
       RowBox[{"{", 
        RowBox[{"0", ",", "1", ",", "2", ",", "3"}], "}"}]}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"Equal", "@@", 
      RowBox[{"test", "\[LeftDoubleBracket]", 
       RowBox[{"1", ";;", "3"}], "\[RightDoubleBracket]"}]}], "||", 
     RowBox[{"Equal", "@@", 
      RowBox[{"test", "\[LeftDoubleBracket]", 
       RowBox[{"2", ";;", "4"}], "\[RightDoubleBracket]"}]}]}]}], 
   "]"}]}]], "Input",ExpressionUUID->"eb8c2dce-b72f-4638-8a75-a37437447e48"],

Cell["The number of hands that contain a straight is", "Text",ExpressionUUID->"62a236a0-ea89-4824-a414-8cb05e90b2d1"],

Cell[BoxData[
 RowBox[{"nf", "=", 
  RowBox[{"Count", "[", 
   RowBox[{"\[CapitalOmega]", ",", 
    RowBox[{"_", "?", 
     RowBox[{"(", "straightQ", ")"}]}]}], "]"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "favorable", " ", "outcomes", " ", "in", " ", "this", " ", "simulation"}], 
   " ", "*)"}]}]], "Input",ExpressionUUID->"9da7c4fa-00bd-42e5-b1c8-\
363fae930b9f"],

Cell["The probability estimate is now", "Text",ExpressionUUID->"42dca201-7f6f-4444-a76d-638140b59607"],

Cell[BoxData[
 RowBox[{"p", "=", 
  RowBox[{
   FractionBox["nf", "n"], "//", "N"}]}]], "Input",ExpressionUUID->"4501759a-\
7c06-4101-a42e-3615fb7635b2"],

Cell[TextData[{
 "We know from above that the correct value is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"17", "/", "285"}], "\[TildeTilde]", 
    RowBox[{"6", "%"}]}], TraditionalForm]],ExpressionUUID->
  "5cd26826-54cc-446c-865e-5567209fc138"],
 ". You are expected to do several simulation in order to view the variation \
(stability) of your estimates. "
}], "Text",ExpressionUUID->"873391e1-1f7d-4c47-8328-5749f2748c50"],

Cell["\<\
Let's introduce three players. A poker round can be simulated by\
\>", "Text",ExpressionUUID->"0ab9cae1-e515-48e9-adf6-cac334916b11"],

Cell[BoxData[
 RowBox[{"play", ":=", 
  RowBox[{"Sort", "/@", 
   RowBox[{"Partition", "[", 
    RowBox[{
     RowBox[{"RandomSample", "[", 
      RowBox[{"deck", ",", 
       RowBox[{"3", "*", "4"}]}], "]"}], ",", "4"}], "]"}]}]}]], "Input",Expre\
ssionUUID->"e52738b6-6f36-4910-a360-9ec2cc273c61"],

Cell["\<\
The code samples 12 cards from the deck and partition the list in three \
sublists with four cards each. Finally, the hands are sorted.\
\>", "Text",ExpressionUUID->"d25cb3d2-c716-4d86-af92-53a8d9e48982"],

Cell[BoxData["play"], "Input",ExpressionUUID->"43e5550e-a6f5-40ef-97fa-f067424eea39"],

Cell["\<\
We can now estimate the probability that at least one player gets a straight.\
\>", "Text",ExpressionUUID->"dd584397-a251-4983-9435-6ce9fa255414"],

Cell[BoxData[
 RowBox[{"play1", "=", "play"}]], "Input",ExpressionUUID->"2cf51136-9d9e-4112-b0be-2a5df5e5ce62"],

Cell["Are there any straights?", "Text",ExpressionUUID->"a7e24d93-3b8e-4ee1-b09c-378c624b9631"],

Cell[BoxData[
 RowBox[{"straightQ", "/@", "play1"}]], "Input",ExpressionUUID->"390b03bc-e52b-40f3-988b-efced068b019"],

Cell[BoxData[
 RowBox[{"Or", "@@", 
  RowBox[{"straightQ", "/@", "play1"}]}]], "Input",ExpressionUUID->"e07b3093-\
e5a7-4890-9e7b-adac27aa5c64"],

Cell["A criteria method is now", "Text",ExpressionUUID->"f1905342-dfad-416c-9818-21f4eb197f82"],

Cell[BoxData[
 RowBox[{
  RowBox[{"straightQ3", "[", "play_", "]"}], ":=", 
  RowBox[{"Or", "@@", 
   RowBox[{"(", 
    RowBox[{"straightQ", "/@", "play"}], ")"}]}]}]], "Input",ExpressionUUID->\
"817204a2-9972-433b-a4da-4b4d5e141f13"],

Cell["1000 games are generated by", "Text",ExpressionUUID->"b8fb5350-ae78-4e19-a275-3b919719009f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"\[CapitalOmega]3", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"play", ",", 
     RowBox[{"{", "1000", "}"}]}], "]"}]}], ";"}]], "Input",ExpressionUUID->\
"da44f51b-0455-4883-a75c-456c14ae10ee"],

Cell["Our probability estimate is", "Text",ExpressionUUID->"bafb9762-6bd8-4b66-bc40-bc9b964986d3"],

Cell[BoxData[
 RowBox[{"p3", "=", 
  FractionBox[
   RowBox[{"Count", "[", 
    RowBox[{"\[CapitalOmega]3", ",", 
     RowBox[{"_", "?", 
      RowBox[{"(", "straightQ3", ")"}]}]}], "]"}], "1000"]}]], "Input",Express\
ionUUID->"22ffef53-c03c-488c-8d59-27d03d9823d7"],

